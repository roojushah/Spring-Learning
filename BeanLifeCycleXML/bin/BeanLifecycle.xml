<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">
	
	<bean id="student" class="com.lifecycle.Student" 
		init-method="myInitMethod" 
		destroy-method="myDestroyMethod" scope="prototype">
		<property name="rollNo" value="1"></property>
		<property name="name" value="Rooju Shah"></property>
		<property name="marks" value="98"></property>
	</bean>
</beans>